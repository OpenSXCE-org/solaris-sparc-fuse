d none usr 755 root sys
d none usr/bin 755 root bin
f none usr/bin/fusermount 755 root bin
d none usr/lib 755 root bin
f none usr/lib/libfuse.so.2.7.1 755 root bin
s none usr/lib/libfuse.so=libfuse.so.2.7.1
