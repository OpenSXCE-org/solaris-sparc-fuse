f none usr/lib/fs/fuse/fusermount.bin 755 root bin
f none usr/lib/fs/fuse/mount 755 root bin
f none usr/lib/fs/fuse/umount 755 root bin
d none usr/lib/pkgconfig  755 root other
f none usr/lib/pkgconfig/fuse.pc 444 root bin
d none usr/include 755 root bin
d none usr/include/fuse 755 root bin
